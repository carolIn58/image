<script type="text/javascript">
    $(document).ready(function(){
        // $("#tenantCode").change(function(){
        //     tenantCode = $(this).val();
        //     changeShow(tenantCode);
        // });

        // 多于一个租户，才出选择框
        if($(".option_group").find("li").length>1){
            $("#tenantSelectDiv").show();
        }

        //设置默认租户
        setDefaultTenant()

        changeForgetHref();
    });
    //重新设置重置密码的链接,加上service参数
    function changeForgetHref(){
        param = getServicePath();
        if(param==null || param==''){
            return;
        }
        tmpLink = $("#forgetLink").attr("href");
        tmpLink = tmpLink + "?service="+decodeURIComponent(param);
        $("#forgetLink").attr("href",tmpLink);
    }
    function getServicePath() {
        var r = location.search.replace("?service=","");
        if (r != null) return unescape(r);
        return null;
    }
    function getQueryString(name) {
        var reg = new RegExp("(^|&)" + name + "=([^&]*)(&|$)", "i");
        var r = window.location.search.substr(1).match(reg);
        if (r != null) return unescape(r[2]);
        return null;
    }

    //设置默认租户
    function setDefaultTenant(){
        lastTenant = getCookie("lasttenant");
        console.info("lasttenant:"+lastTenant)
        //有上次登录登录的租户cookie
        if(lastTenant != null){//设置上次选择租户
            if($(".option_group").find("li[value='"+lastTenant+"']").length>0){
                console.info("last："+lastTenant)
                changeTenant(lastTenant)
                return;
            }
        }
        //有58默认58,没有设第一个
        if($(".option_group").find("li[value='58']").length>0){
            console.info("default 58")
            changeTenant("58");
        }else{
            tmpTenant = $('.option_group li:first').attr("value");
            console.info("first tenant："+tmpTenant)
            changeTenant(tmpTenant);
        }
    }

    function changeTenant(tenantCode) {
        changeShow(tenantCode)
        // $("#tenantCode").find("option[value='"+lastTenant+"']").attr("selected",true);
        $(".option_group").find("li[value='"+tenantCode+"']").click();
        $(document).click();
    }

    //改变显示信息
    function changeShow(tenantCode) {
        //logo和文字显示
        // icon = $(".option_group").find("li[value='"+tenantCode+"']").attr("icon");
        // tenantName = ($(".option_group").find("li[value='"+tenantCode+"']").html());

        //改变logo和文字
        // $("#logoImg").css("background-image","url("+icon+")");
        // $("#login").html(tenantName+"统一登录平台");
    }

    function getCookie(name) {
        var prefix = name + "="
        var start = document.cookie.indexOf(prefix)
        if (start == -1) {
            return null;
        }
        var end = document.cookie.indexOf(";", start + prefix.length)
        if (end == -1) {
            end = document.cookie.length;
        }
        var value = document.cookie.substring(start + prefix.length, end)
        return unescape(value);
    }

    //切到账号登录
    $(".click-back-account").on("click",function(){
        $(".account-b").click();
    });

    //模拟select
    $(".select_open,.select_txt").on("click",function(event){
        event.stopPropagation();
        $(".option_group").toggle();
        // $(".option_group").hide();
    });
    /*赋值给文本框*/
    $(".option").on("click",function(){
        var text=$(this).text();
        $(this).parent().siblings(".select_txt").text(text);

        var value=$(this).attr("value");
        changeShow(value);
        $("#tenantCode").val(value)
        $(".option_group").hide();

        //高亮选择的option(加粗)
        $(this).siblings().removeClass("bgg");
        $(this).addClass("bgg");
    })
    $(document).on("click",function(event){
        var eo=$(event.target);
        if($(".select_open,.select_txt").is(":visible") && eo.attr("class")!="option_group" && !eo.parent(".option_group").length){
            $('.option_group').hide();
        }
    });

    var options = {
        excludeColorDepth: true,
        excludeScreenResolution: true,
        excludeAvailableScreenResolution: true,
        excludeAddBehavior: true,
        excludeDoNotTrack: true,
        excludeAdBlock: true,
        excludeJsFonts: true,
        excludePlugins: true,
        excludeIEPlugins: true,
        excludeTouchSupport: true
    };

    new Fingerprint2(options).get(function (result) {
        document.getElementById("fingerPrint").value = result;
    });

    function login() {
        $.ajax({
            type: "post",
            url: "resources/getPublicKey",
            data: "",
            async: false,
            success: function (key) {
                var encryption = new JSEncrypt();
                encryption.setPublicKey(key);
                var encryptedPassword = encryption.encrypt($("#password").val());
                $("#password").val(encryptedPassword);

                //保存到cookie并设置有效时间为15天:
                document.cookie = "lasttenant=" + $("#tenantCode").val() + ";expires=15;Path=/";

                $("#fm1").submit();
            }
        });
    }

    // NOTE: window.RTCPeerConnection is "not a constructor" in FF22/23
    var RTCPeerConnection = /*window.RTCPeerConnection ||*/ window.webkitRTCPeerConnection || window.mozRTCPeerConnection;

    if (RTCPeerConnection) (function () {
        var rtc = new RTCPeerConnection({iceServers: []});
        if (1 || window.mozRTCPeerConnection) {      // FF [and now Chrome!] needs a channel/stream to proceed
            rtc.createDataChannel('', {reliable: false});
        }
        ;

        rtc.onicecandidate = function (evt) {
            // convert the candidate to SDP so we can run it through our general parser
            // see https://twitter.com/lancestout/status/525796175425720320 for details
            if (evt.candidate) grepSDP("a=" + evt.candidate.candidate);
        };

        rtc.createOffer(function (offerDesc) {
            grepSDP(offerDesc.sdp);
            rtc.setLocalDescription(offerDesc);
        }, function (e) {
            console.warn("offer failed", e);
        });

        var addrs = Object.create(null);
        addrs["0.0.0.0"] = false;

        function updateDisplay(newAddr) {
            if (newAddr in addrs) return;
            else addrs[newAddr] = true;
            var displayAddrs = Object.keys(addrs).filter(function (k) {
                return addrs[k];
            });
            //10.252.55.91 or perhaps fe80::ffff:ffff:fffe
            var ipAddr = displayAddrs.join(" or perhaps ") || "n/a";
            document.getElementById("realIp").value = ipAddr;
        }

        function grepSDP(sdp) {
            var hosts = [];
            sdp.split('\r\n').forEach(function (line) { // c.f. http://tools.ietf.org/html/rfc4566#page-39
                if (~line.indexOf("a=candidate")) {     // http://tools.ietf.org/html/rfc4566#section-5.13
                    var parts = line.split(' '),        // http://tools.ietf.org/html/rfc5245#section-15.1
                        addr = parts[4],
                        type = parts[7];
                    if (type === 'host') updateDisplay(addr);
                } else if (~line.indexOf("c=")) {       // http://tools.ietf.org/html/rfc4566#section-5.7
                    var parts = line.split(' '),
                        addr = parts[2];
                    updateDisplay(addr);
                }
            });
        }

    })(); else {
        //console.log("ifconfig | grep inet | grep -v inet6 | cut -d\" \" -f2 | tail -n1");
        //console.log("In Chrome and Firefox your IP should display automatically, by the power of WebRTC skill.");
    }

    var qrCodeServer = null;

    function getQrCodeServer() {
        $.ajax({
            type: "post",
            url: "resources/getQrCodeServer",
            data: "",
            async: false,
            success: function (address) {
                qrCodeServer = address;
            }
        });
    }

    var qrCodeTask = null;

    var requestId = null;
    getRequestId(true);

    function getRequestId(async) {
        if (requestId === null) {
            $.ajax({
                type: "post",
                url: "resources/getQrCodeRequestId",
                data: "",
                async: async,
                success: function (result) {
                    var data = JSON.parse(result);
                    if (data.success === false || data.code !== 0) {
                        console.error("获取requestId异常：" + data.msg);
                        return;
                    }
                    requestId = data.data;
                }
            });
        }
    }

    function qrCode() {
        getRequestId(false);
        if (qrCodeServer === null) {
            getQrCodeServer();
        }

        $("#qrCode").attr('src', qrCodeServer + "/api/QRCode?requestId=" + requestId);


        stopInterval();

        qrCodeTask = setInterval(function () {
            var blackBox = document.getElementById("blackBox").value
            var service = getQueryString("service")
            $.ajax({
                type: "post",
                url: "resources/getTgc",
                data: {"requestId": requestId,"blackBox":blackBox,"service":service},
                async: true,
                success: function (cookieResult, success) {
                    if (success) {

                        var cookieData = JSON.parse(cookieResult);
                        if (cookieData.success === false) {
                            console.error("获取二维码扫描结果异常：" + cookieData.msg);
                            //获取失败后继续获取 add by wangchao53
                            requestId = null;
                            stopInterval();
                            return;
                        }

                        if (cookieData.code === 1) {
                            return;
                        }

                        if (cookieData.code === 2) {
                            console.error("二维码已过期：" + cookieData.msg);
                            requestId = null;
                            stopInterval();
                            console.log("更新二维码")
                            qrCode();
                        }

                        if (typeof (cookieData.data) !== 'undefined' && cookieData.data !== '') {
                            requestId = null;
                            stopInterval();
                            setCookie("TGC", cookieData.data);
                        }
                    }
                },
                error: function () {
                    console.error("获取二维码扫描结果异常");
                    stopInterval();
                }
            });
        }, 1500);
    }

    function stopInterval() {
        if (qrCodeTask !== null) {
            clearInterval(qrCodeTask);
            qrCodeTask = null;
        }
    }

    function setCookie(name, value) {

        $.ajax({
            type: "post",
            url: "resources/getQrCodeTgtLiveTime",
            data: "",
            async: true,
